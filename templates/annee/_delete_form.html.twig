<form method="post" action="{{ path('backend_annee_delete', {'id': annee.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer la date?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ annee.id) }}">
    <button class="btn btn-danger">{{ 'btnDelete'|trans }}</button>
</form>
